#include<stdio.h>
int main()
{
    int a,b,temp;
    printf("Enter two number:");
    scanf("%d %d",&a,&b);                                                               ,&a,&b);
    //swapping
    temp =a;
    a=b;
    b=temp;
    printf("After swapping:\n");
    printf("a=%d\n",a);
    printf("b=%d\n",b);
    return 0;
}

